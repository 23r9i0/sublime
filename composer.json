{
	"name": "kallookoo/sublime",
	"type": "wordpress-plugin",
	"homepage": "https://github.com/kallookoo/sublime",
	"license": "GPL-2.0+",
	"require": {
		"php"                            : ">=5.6",
		"composer/installers"            : "~1.0",
		"phpdocumentor/reflection"       : "~3.0",
		"erusev/parsedown"               : "~1.7",
		"psr/log"                        : "~1.0",
		"scribu/lib-posts-to-posts"      : "dev-master@dev",
		"scribu/scb-framework"           : "dev-master@dev",
		"wordpress/phpdoc-parser"        : "dev-master@dev"
	},
	"autoload": {
		"classmap": ["classes"],
		"files": ["config/command.php", "config/import.php", "config/export.php"]
	},
	"extra": {
		"installer-paths": {
			"vendor/wordpress/phpdoc-parser" : ["wordpress/phpdoc-parser"]
		}
	}
}